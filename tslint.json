{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "max-classes-per-file": false,
        "new-parens": false,
        "no-unused-expression": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "trailing-comma": [true, {"esSpecCompliant": true}]
    },
    "rulesDirectory": []
}